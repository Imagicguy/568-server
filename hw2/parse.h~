#ifndef __PRASE_H__
#define __PRASE_H__
#include "proxy.h"

class Parse {
private:
  std::string request;
  std::string req_line;
  std::string type;
  std::string hostname;
  std::string port;

public:
  Parse() {}
  void init_connect(std::string request, std::string line);
  void parse_type(std::string line);
  void parse_hostname(std::string line);
  void parse_port(std::string line);
};
#endif
